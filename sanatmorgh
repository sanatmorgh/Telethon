from telethon.sync import TelegramClient
api_id = 1278321
api_hash = 'ce284199114b692627fe9e85680cafd5'
phone = '+989120144108'
client = TelegramClient(+989120144108, 1278321, ce284199114b692627fe9e85680cafd5)
client.connect()
if not client.is_user_authorized():
    client.send_code_request(phone)
    client.sign_in(phone, input('Enter the code: '))


